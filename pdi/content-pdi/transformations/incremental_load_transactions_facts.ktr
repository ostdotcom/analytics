    <sql>drop table if exists temp_incremental_token_transfer_facts_${CHAIN_ID};

create table temp_incremental_token_transfer_facts_${CHAIN_ID} as
(select 
		t.time_sk, 
		d.date_sk, 
		tok.token_sk, 
 		IFNULL(mt.meta_type_sk, 0) as meta_type_sk, 
 		IFNULL(mn.meta_name_sk, 0) as meta_name_sk, 
		a.final_status,
       total_transactions AS total_transactions,
       total_transfers AS total_transfers,
       total_volume AS total_volume,
       total_gas_used AS total_gas_used	
FROM incremental_aggregated_transfers_details_${CHAIN_ID} as a
left join dim_dates as d
on a.rounded_date_timestamp = d.timestamp
left join dim_meta_names_${CHAIN_ID} as mn
on a.meta_name = mn.meta_name
and a.token_id = mn.token_id
left join dim_meta_types as mt
on a.meta_type = mt.meta_type
left join dim_times as t
on a.rounded_time_timestamp = t.timestamp
left join dim_tokens_${CHAIN_ID} as tok
on a.token_id = tok.token_id)
;


drop table if exists temp_incremental_token_transfer_facts_details_${CHAIN_ID};

create table temp_incremental_token_transfer_facts_details_${CHAIN_ID} as
select IFNULL(min(date_sk), 2000000000) as min_date_sk from temp_incremental_token_transfer_facts_${CHAIN_ID};


SET autocommit=0;
START TRANSACTION;

DELETE
FROM token_transfer_facts_${CHAIN_ID}
WHERE (date_sk >(SELECT min_date_sk
                 FROM temp_incremental_token_transfer_facts_details_${CHAIN_ID}))
OR    (date_sk = (SELECT min_date_sk
                  FROM temp_incremental_token_transfer_facts_details_${CHAIN_ID}) AND time_sk >= (SELECT MIN(time_sk) AS min_time_sk
                                                                                                  FROM temp_incremental_token_transfer_facts_${CHAIN_ID}
                                                                                                  WHERE date_sk = (SELECT min_date_sk
                                                                                                                   FROM temp_incremental_token_transfer_facts_details_${CHAIN_ID})));



INSERT into token_transfer_facts_${CHAIN_ID}(time_sk, date_sk, token_sk, meta_type_sk, meta_name_sk, status, total_transactions, total_transfers, total_volume, total_gas_used)
(select * from temp_incremental_token_transfer_facts_${CHAIN_ID} order by date_sk desc , time_sk desc, token_sk);

commit;
SET autocommit=1;

drop table temp_incremental_token_transfer_facts_${CHAIN_ID};
drop table temp_incremental_token_transfer_facts_details_${CHAIN_ID};
</sql>
