    <connection>analytics_redshift_connection</connection>
    <sql>SELECT *
FROM (
(
SELECT COALESCE(SUM(gas_used),0) AS value,
       'total_gas_used' AS metric_name
FROM ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transactions_${CHAIN_ID}
WHERE kind = 54
AND   token_id IS NOT NULL
AND   token_id > 0
)
UNION ALL
(
SELECT COUNT(DISTINCT tx_hash) AS value,
       'total_distinct_transactions' AS metric_name
FROM ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transactions_${CHAIN_ID}
WHERE kind = 54
AND   token_id IS NOT NULL
AND   token_id > 0
)
UNION ALL
(
SELECT COALESCE(SUM(b.amount),0) AS value,
       'total_volume' AS metric_name
FROM ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transactions_${CHAIN_ID} AS a
  LEFT OUTER JOIN ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transfers_${CHAIN_ID} AS b ON a.tx_hash = b.tx_hash
WHERE a.kind = 54
AND   a.token_id IS NOT NULL
AND   a.token_id > 0
)
UNION ALL
(
SELECT COUNT(DISTINCT b.tx_hash || '-' || b.event_index) AS value,
       'total_transfers' AS metric_name
FROM ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transactions_${CHAIN_ID} AS a
  LEFT OUTER JOIN ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transfers_${CHAIN_ID} AS b ON a.tx_hash = b.tx_hash
WHERE a.kind = 54
AND   a.token_id IS NOT NULL
AND   a.token_id > 0

)
UNION ALL
(
SELECT COUNT(1) AS value,
       'duplicate_transaction_count' AS metric_name
FROM (SELECT tx_hash
      FROM ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transactions_${CHAIN_ID}
      GROUP BY 1
      HAVING COUNT(1) > 1
      )
)
UNION ALL
(
SELECT COUNT(1) AS value,
       'duplicate_transfer_count' AS metric_name
FROM (SELECT tx_hash,
             event_index
      FROM ${PRESTAGING_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.transfers_${CHAIN_ID}
      GROUP BY 1,
               2
      HAVING COUNT(1) > 1)
      
)
)
ORDER BY 2;</sql>
