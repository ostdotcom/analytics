    <sql>
SELECT *
FROM (
(
SELECT COALESCE(SUM(total_transactions),0) AS value,  
'workflow_steps_transactions_count' AS metric_name 
FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.workflow_facts 
where workflow_kind_sk not in (SELECT workflow_kind_sk FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.dim_workflow_kinds 
							where kind in ( 500 , 0)) and  workflow_date_sk &lt; ${END_DATE_SK} and  workflow_date_sk >= ${VERIFICATION_START_DATE_SK}


)
UNION ALL
(
select COALESCE(count(distinct workflow_id),0) as value, 
'workflow_success_count' AS metric_name 
FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.all_workflows_transactions where workflow_status  = 'success'
and workflow_kind_sk not in (SELECT workflow_kind_sk FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.dim_workflow_kinds 
							where kind in ( 500, 0 )) 
and  workflow_date_sk &lt; ${END_DATE_SK} and  workflow_date_sk >= ${VERIFICATION_START_DATE_SK}
)
UNION ALL
(
select COALESCE(count(distinct workflow_id),0) as value, 
'workflow_failed_count' AS metric_name 
FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.all_workflows_transactions where workflow_status  = 'fail'
and workflow_kind_sk not in (SELECT workflow_kind_sk FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.dim_workflow_kinds 
							where kind in ( 500 , 0)) 
and  workflow_date_sk &lt; ${END_DATE_SK} and  workflow_date_sk >= ${VERIFICATION_START_DATE_SK}
)
UNION ALL
(
select coalesce(count(1),0) as value, 
'duplicate_addresses_count' AS metric_name 
FROM (select address from ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.workflow_addresses  
group by address 
having count(1) > 1)
)
UNION ALL
(
select count(*) as value, 
'pc_addresses_count' AS metric_name 
FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.workflow_addresses 
where address_type_sk = (select address_type_sk from ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.dim_address_types where address_type = 'PARTNER COMPANY')
)
UNION ALL
(
select count(*) as value, 
'unknown_workflow_kind_or_address_type_count' AS metric_name  
FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.workflow_facts 
where workflow_kind_sk = 0
or from_address_type_sk = 0
)
UNION ALL
(
select COALESCE(count(distinct workflow_id),0) as value, 
'all_workflows_count' AS metric_name  
FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.all_workflows_transactions  
where workflow_kind_sk not in (SELECT workflow_kind_sk FROM ${PENTAHO_REDSHIFT_SCHEMA_PREFIX}_${SUB_ENV}${ENV_SUFFIX}.dim_workflow_kinds 
							where kind in ( 500 , 0)) and  workflow_date_sk &lt; ${END_DATE_SK}  and  workflow_date_sk >= ${VERIFICATION_START_DATE_SK}
)

)
ORDER BY 2;

</sql>
